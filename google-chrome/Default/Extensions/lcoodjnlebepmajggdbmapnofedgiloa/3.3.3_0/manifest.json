{
   "action": {
      "default_icon": {
         "128": "icon.png"
      },
      "default_popup": "options/options.html",
      "default_title": "UsableNet AQA"
   },
   "author": "Manuel Rizzi \u003Cmanuel.rizzi@usablenet.com>",
   "background": {
      "service_worker": "background/background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content_scripts/inspectFrame.js" ],
      "match_about_blank": true,
      "match_origin_as_fallback": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "content_scripts/shadow-root-init.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start",
      "world": "MAIN"
   } ],
   "description": "Extend AQA Accessibility into Chrome",
   "devtools_page": "devtools/devtools.html",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "UsableNet AQA",
   "optional_permissions": [ "downloads" ],
   "options_ui": {
      "page": "options/options.html"
   },
   "permissions": [ "storage", "webNavigation", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.3.3",
   "version_name": "3.3.3",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "content_scripts/inspectFrame.js", "content_scripts/shadow-root-init.js" ]
   } ]
}
